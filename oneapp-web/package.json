{
  "name": "oneapp-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:staging": "vue-cli-service build --mode staging",
    "test:unit": "vue-cli-service test:unit",
    "lint:pre": "vue-cli-service lint --no-fix",
    "lint": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.17.3",
    "core-js": "^3.6.5",
    "fuse.js": "^6.4.1",
    "lodash": "^4.17.20",
    "standard-version": "^9.0.0",
    "uswds": "^2.8.1",
    "vee-validate": "^3.4.0",
    "vue": "^2.6.12",
    "vue-i18n": "^8.21.1",
    "vue-router": "^3.4.3",
    "vue-the-mask": "^0.11.1",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.4",
    "@vue/cli-plugin-eslint": "~4.5.6",
    "@vue/cli-plugin-router": "~4.5.4",
    "@vue/cli-plugin-unit-jest": "~4.5.6",
    "@vue/cli-plugin-vuex": "~4.5.4",
    "@vue/cli-service": "~4.5.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "callsite": "^1.0.0",
    "chalk": "^4.1.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "knex": "^0.21.5",
    "node-sass": "^4.12.0",
    "oracledb": "^5.0.0",
    "prettier": "^2.1.1",
    "sass-loader": "^10.0.2",
    "sqlite3": "^5.0.0",
    "vue-template-compiler": "^2.6.12",
    "winston": "^3.3.3"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off",
      "vue/no-unused-vars": "off",
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
